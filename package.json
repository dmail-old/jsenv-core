{
	"name": "system-run",
	"version": "0.0.1",
	"description": "Run JavaScript everywhere",
	"license": "UNLICENSED",
	"preferGlobal": true,
	"dependencies": {
		"babel-core": "^5.8.22",
		"babel-polyfill": "6.0.16",
		"systemjs": "0.19.27",
		"source-map": "0.4.4",
		"@dmail/node-stacktrace": "git+https://github.com/dmail/node-stacktrace",
		"open": "*"
	},
	"bin": {
		"jsenv": "./bin/run"
	},
	"scripts": {
		"install-dev": "node ./bin/install-dev",
		"link-dev": "npm link ../node-stacktrace && npm link ../promise && npm link ../set-immediate && npm link ../url && npm link ../url-search-params && npm link ../rest && npm link ../service-http",
		"test": "node ./bin/run test/modules/module-test.js -test",
		"cover": "node ./bin/run test/modules/module-test.js -test -cover",
		"cover-console": "node ./bin/run test/modules/module-test.js -test -cover -cover-console",
		"cover-html": "node ./bin/run test/modules/module-test.js -test -cover -cover-console -cover-html",
		"cover-json": "node ./bin/run test/modules/module-test.js -test -cover -cover-console -cover-json",
		"ci": "node ./bin/run test/modules/module-test.js -test -cover -cover-codecov -cover-codecov-token=f695edf1-d0f4-4799-bb08-46955137f0c3"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/dmail/system-run"
	}
}
